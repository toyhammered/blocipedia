<div class="col-lg-3 col-sm-6 col-xs-12">
  <%= link_to wiki_path(wiki.id) do %>
  <div class="dummy"></div>
  <div class="panel panel-default square">
    <% if wiki.private? %>
      <i class="fa fa-2x fa-lock pull-right" id="private-wiki"></i>
    <% end %>
    <h3 class="text-center"><%= wiki.title %></h3>
  </div>
  <% end %>
</div>
