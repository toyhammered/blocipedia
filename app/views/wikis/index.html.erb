<div class="row">
    <div class="panel panel-heading">
        <h2 class="text-center">All Public Wikis</h2>
    </div>
</div>
<div class="row">
    <!-- new wiki -->
    <div class="col-lg-3 col-sm-6 col-xs-12">
      <%= link_to new_wiki_path do %>
      <div class="dummy"></div>
      <div class="panel panel-default square">
        <h3 class="text-center"><i class="fa fa-plus-circle"></i></h3>
      </div>
      <% end %>
    </div>

    <!-- database wikis -->
    <%= render partial: 'wikis/wiki', collection: @wikis %>
</div>
